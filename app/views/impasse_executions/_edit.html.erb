<% if Rails::VERSION::MAJOR < 3 %>
  <% labelled_form_for :execution, @execution, {} do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
  <% end %>
<% else %>
  <% output = labelled_form_for @execution, :as => :execution, :url => { :action => :edit } do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
  <% end %>
  <%= output %>
<% end %>
