<% if Rails::VERSION::MAJOR < 3 %>
  <% labelled_form_for :test_plan, @test_plan, {} do |f| %>
    <%= render :partial => "form", :locals => {:form => f} %>
    <%= submit_tag l(:button_create) %>
  <% end %>
<% else %>
  <% output = labelled_form_for @test_plan, :as => :test_plan, :url => { :action => :new }  do |f| %>
    <%= render :partial => "form", :locals => {:form => f} %>
    <%= submit_tag l(:button_create) %>
  <% end %>
  <%= output %>
<% end %>
